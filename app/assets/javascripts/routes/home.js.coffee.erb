
app.config [
  "$routeProvider"
  "$locationProvider"
  (
    $routeProvider
    $locationProvider
  ) ->
    $locationProvider.html5Mode(true)
    $routeProvider
      .when("/",
        templateUrl: "<%= asset_path("home/index.html") %>"
        controller: "HomeCtrl"
      )
      .otherwise redirectTo: "/"
]